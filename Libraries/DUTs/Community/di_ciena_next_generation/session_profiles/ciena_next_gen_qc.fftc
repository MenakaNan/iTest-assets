                        <body>$continue_login</body>
                    </command>
                    <nestedSteps>
                        <item guid="b948ca4a-747a-4090-ba6a-aabf5ca2834f" action="then">
                            <nestedSteps>
                                <item guid="f3c3f375-93ae-46f3-8113-2aeeb5857a76" action="comment">
