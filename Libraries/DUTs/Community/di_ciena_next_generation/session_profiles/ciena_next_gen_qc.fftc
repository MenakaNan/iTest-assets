                                                                <actionProperties type="com.fnfr.svt.execution.builtin.actions.ExecutionIssuePropertyGroup" severity="Information">
                                                                    <message>CFM service ${md}$ma reporting NO SERVICE FAULTS for $session.</message>
                                                                </actionProperties>
